{
  "name": "project-template",
  "version": "1.0.0",
  "description": "basic template",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-s build:clean build:server build:client",
    "build:clean": "rimraf temp && mkdirp dist",
    "build:server": "babel --ignore ./src/client src -d dist",
    "build:client": "webpack --mode production",
    "start": "concurrently \"yarn start:app\" \"yarn start:webpack\"",
    "start:app": "nodemon --ext js,jsx,json,html --exec babel-node src/expressServer.js",
    "start:webpack": "webpack serve"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.12.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.10.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  }
}
